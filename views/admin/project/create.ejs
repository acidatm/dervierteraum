<h1>Create Project</h1>
<%- include ('../partials/messages') %>
<form action="/project/create" method="POST">
  <div>
      <label for="name">Project Name</label>
      <input type="text" name="projectname" placeholder="Enter Project Name" value="<%= typeof projectname != 'undefined' ? projectname : '' %>"/>
  </div>
  <button type="submit">Create</button>
  <a class="warn" href="/admin">Cancel</a>
</form>
