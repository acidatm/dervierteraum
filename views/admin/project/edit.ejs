<div id="left">
<h1>Edit Project "<%= project.projectname %>" as Admin</h1>
<%- include ('../../partials/messages') %>
<form action="/admin/project/update" method="POST">
  <input style="display:none;pointer-events:none;width:0;height:0;visible:false;" type="text" name="projectname" value="<%= typeof project.projectname != 'undefined' ? project.projectname : '' %>"/>
  <div>
    <h2>Administration</h2>
    <div>
        <label for="admin">Admin</label>
        <input type="text" name="admin"
        value="<%= typeof project.admin != 'undefined' ? project.admin : '' %>"/>
    </div>
    <div>
        <label for="enabled">Enabled</label>
        <input type="checkbox" name="enabled"
        <% if(project.enabled){ %>checked<% } %>
        value="true"/>
    </div>
    <div>
        <label for="x">X Position</label>
        <input type="number" name="x"
        value="<%= typeof project.x != 'undefined' ? project.x : '' %>"/>
    </div>
    <div>
        <label for="y">Y Position</label>
        <input type="number" name="y"
        value="<%= typeof project.y != 'undefined' ? project.y : '' %>"/>
    </div>
  </div>
  <div>
    <h2>Settings</h2>
    <div>
        <label for="visible">Visible</label>
        <input type="checkbox" name="visible"
        <% if(project.visible){ %>checked<% } %>
        value="true"/>
    </div>
  </div>
  <div>
    <h2>Metadata</h2>
    <div>
        <label for="title">Title</label>
        <input type="text" name="title"
        value="<%= typeof project.title != 'undefined' ? project.title : '' %>"/>
    </div>
    <div>
        <label for="path">Path</label>
        <input type="text" name="path"
        value="<%= typeof project.path != 'undefined' ? project.path : '' %>"/>
    </div>
    <div>
        <label for="description">Description</label>
        <input type="text" name="description"
        value="<%= typeof project.description != 'undefined' ? project.description : '' %>"/>
    </div>
    <div>
        <label for="artists">Artists</label>
        <input type="text" name="artists"
        value="<%= typeof project.artists != 'undefined' ? project.artists : '' %>"/>
    </div>
    <div>
        <label for="date">Date</label>
        <input type="text" name="date"
        value="<%= typeof project.date != 'undefined' ? project.date : '' %>"/>
    </div>
    <div>
        <label for="link">Link</label>
        <input type="text" name="link"
        value="<%= typeof project.link != 'undefined' ? project.link : '' %>"/>
    </div>
    <div>
        <label for="linktitle">Linktitle</label>
        <input type="text" name="linktitle"
        value="<%= typeof project.linktitle != 'undefined' ? project.linktitle : '' %>"/>
    </div>
    <input style="display:none;pointer-events:none;width:0;height:0;visible:false;" id="config" type="text" name="config" value="<%= typeof project.config != 'undefined' ? project.config : '' %>"/>
  </div>
  <div>
    <h2>Pillar</h2>
    <div id="editor"></div>
  </div>
  <button type="submit">Update</button><a class="warn" href="/admin">Cancel</a>
</form>
<br>
<br>
<br>
</div>
<canvas id="canvas" class="zdog-canvas" width="240" height="240"></canvas>
<script src="/static/js/libs.js"></script>
<script src="/static/js/edit.js"></script>
